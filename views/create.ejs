<form action="/create" method="post" data-submitTo=".body" class="cf">
    <input type="text" id="shrNick" name="nick" placeholder="your Nickname" />
    <input type="text" id="shrName" name="title" placeholder="Title" />
    <select name="type" id="shrType" style="float:right;">
        <option value="paste">paste</option>
        <option value="image">image</option>
    </select>
    <div class="frm">
        <%- partial('tmpl/paste/new') %>
    </div>
    <input type="submit" value="paste"  style="float: right;">
</form>
